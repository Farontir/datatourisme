        }
      ]
    },
    {
      "name": "Profil",
      "short_name": "Profil",
      "description": "Gérer mon profil",
      "url": "/profile",
      "icons": [
        {
          "src": "/icons/shortcut-profile.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+datatourisme",
      "url": "/handle?url=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/open-ics",
      "accept": {
        "text/calendar": [".ics"]
      }
    }
  ]
}